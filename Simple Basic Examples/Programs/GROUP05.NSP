  MOVE GROUP1.#A8 TO GROUP2.#A8
  MOVE GROUP1.#A9 TO GROUP2.#A9
  MOVE GROUP1.#A10 TO GROUP2.#A10
END-FOR
*
COMPUTE #CPU-ELAPSED = *CPU-TIME - #CPU-START
WRITE 5T 'MULTI MOVES:' *TIMD (SETD.) #CPU-ELAPSED
*
*
SETE. SETTIME
MOVE *CPU-TIME TO #CPU-START
*
FOR #LOOP = 1 TO 1000000
  IGNORE
END-FOR
*
COMPUTE #CPU-ELAPSED = *CPU-TIME - #CPU-START
WRITE 5T 'DUMMY FOR:' *TIMD (SETE.) #CPU-ELAPSED
*
END



